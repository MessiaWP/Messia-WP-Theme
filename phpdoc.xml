<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
        configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://www.phpdoc.org"
        xsi:noNamespaceSchemaLocation="data/xsd/phpdoc.xsd"
>
    <title>WP Messia Theme</title>
    <paths>
        <output>../_phpdoc/messia</output>
		<cache>../_phpdoc/.cache</cache>
    </paths>
    <version number="3.0.0">
        <folder>latest</folder>
        <api>
            <source dsn=".">
                <path>wp-content/themes/messia/</path>
            </source>
            <output>api</output>
            <ignore hidden="true" symlinks="true">
                <path>tests/**/*</path>
                <path>build/**/*</path>
                <path>var/**/*</path>
                <path>vendor/**/*</path>
                <path>wp-content/themes/messia/includes/phpwee</path>
            </ignore>
            <extensions>
                <extension>php</extension>
            </extensions>
            <ignore-tags>
                <ignore-tag>template</ignore-tag>
                <ignore-tag>template-extends</ignore-tag>
                <ignore-tag>template-implements</ignore-tag>
                <ignore-tag>extends</ignore-tag>
                <ignore-tag>implements</ignore-tag>
            </ignore-tags>
            <default-package-name>Messia</default-package-name>
        </api>
        <guide>
            <source dsn=".">
                <path>docs</path>
            </source>
            <output>guide</output>
        </guide>
    </version>
    <template name="default"/>
	<!-- <template name="clean"/> -->
</phpdocumentor>